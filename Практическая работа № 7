#include <bits/stdc++.h>
using namespace std;

int main() {
    int x;
    
    cout << "Введите число: ";
    cin >> x;
    
    int original = x;
    int reversed = 0;
    
    while (x > 0) {
        int digit = x % 10;        
        reversed = reversed * 10 + digit;   
        x = x / 10;                  
    }
    bool isPalindrome = (original == reversed);
    
    cout << boolalpha << isPalindrome << endl;
    
    return 0;
}
