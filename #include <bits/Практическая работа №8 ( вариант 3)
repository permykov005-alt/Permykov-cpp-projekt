#include <bits/stdc++.h>
using namespace std;

int main() {
    string word;
    cout << "Введите строку: ";
    getline(cin, word);
    for (int i = 0; i < word.length(); i++) {
    
        if (word[i] == ' ') {
            continue;
        }
        for (int j = i + 1; j < word.length(); j++) {
            if (word[j] == ' ') {
                continue;  
            }
            
            if (word[i] == word[j]) {
                cout << "false" << endl;
                return 0;
            }
        }
    }
    cout << "true" << endl;
    return 0;
}
