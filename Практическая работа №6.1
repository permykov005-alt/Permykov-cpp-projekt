#include <bits/stdc++.h>
using namespace std;

int main() {
    const int SIZE = 5;
    int arr[SIZE];
    
    srand(time(0));
    
    for (int i = 0; i < SIZE; i++) {
        arr[i] = rand() % 10 + 1;  
    }
    
    cout << "Элементы массива:" << endl;
    for (int i = 0; i < SIZE; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;
    
    int max = arr[0];
    for (int i = 1; i < SIZE; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }
    
    cout << "Максимальный элемент: " << max << endl;
    
    return 0;
}
