#include <bits/stdc++.h>
using namespace std;
int main() {
    vector<int> nums = {3, 3};
    int target = 6;
    
    unordered_map<int, int> num_map;
    
    for (int i = 0; i < nums.size(); i++) {
        int complement = target - nums[i];

        if (num_map.find(complement) != num_map.end()) {
          
            cout << num_map[complement] << ", " << i << endl;
            return 0;
        }
        num_map[nums[i]] = i;
    }
    return 0;
}
