#include <bits/stdc++.h>
using namespace std;

int main() {
    const int ROWS = 2;
    const int COLS = 3;
    int arr[ROWS][COLS];
    int num = 1;
    for (int i = 0; i < ROWS; i++) {
        for (int j = 0; j < COLS; j++) {
            arr[i][j] = num++;
        }
    }
    cout << "Элементы массива:" << endl;
    for (int i = 0; i < ROWS; i++) {
        for (int j = 0; j < COLS; j++) {
            cout << arr[i][j] << " ";
        }
        cout << endl; 
    }
    
    return 0;
}
